{
  "name": "env",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        }
      }
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "_spacing": {
      "type": "PATTERN",
      "value": "[ \\t]*"
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_double"
        },
        {
          "type": "SYMBOL",
          "name": "string_single"
        },
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "uri"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "SYMBOL",
          "name": "raw_value"
        }
      ]
    },
    "raw_value": {
      "type": "PATTERN",
      "value": "[^\"'\\n\\r][^\\n\\r]*"
    },
    "string_double": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "SYMBOL",
                "name": "interpolation"
              },
              {
                "type": "PATTERN",
                "value": "[^\"$\\\\\\\\]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "string_single": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^']+"
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "interpolation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "interpolation_default"
        },
        {
          "type": "SYMBOL",
          "name": "interpolation_simple"
        },
        {
          "type": "SYMBOL",
          "name": "interpolation_short"
        }
      ]
    },
    "interpolation_default": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "${"
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "STRING",
                  "value": "-"
                }
              ]
            }
          },
          {
            "type": "FIELD",
            "name": "default",
            "content": {
              "type": "SYMBOL",
              "name": "interpolation_value"
            }
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "interpolation_simple": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "interpolation_short": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "interpolation_value": {
      "type": "PATTERN",
      "value": "[^}]+"
    },
    "escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\."
    },
    "identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_.-]*"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "@"
            },
            {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_-]+"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_-]+"
            }
          ]
        }
      ]
    },
    "bool": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "true"
            },
            {
              "type": "STRING",
              "value": "false"
            }
          ]
        }
      }
    },
    "integer": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[+-]?\\d+"
        }
      }
    },
    "url": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 3,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "https?:\\/\\/"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[a-zA-Z0-9._~!$&'()*+,;=:-]+"
                    },
                    {
                      "type": "STRING",
                      "value": "@"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9.-]+"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ":"
                    },
                    {
                      "type": "PATTERN",
                      "value": "\\d+"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\/[^\\s#]*"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "#"
                    },
                    {
                      "type": "PATTERN",
                      "value": "[^\\s]*"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      }
    },
    "uri": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "https?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "wss?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "postgres(ql)?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "mysql:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "mongodb:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "redis:\\/\\/"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "jdbc:"
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "mysql"
                        },
                        {
                          "type": "STRING",
                          "value": "postgresql"
                        },
                        {
                          "type": "STRING",
                          "value": "sqlite"
                        }
                      ]
                    },
                    {
                      "type": "STRING",
                      "value": "://"
                    }
                  ]
                },
                {
                  "type": "PATTERN",
                  "value": "file:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "ftps?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "sftp:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "git(\\+https?|\\+ssh)?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "ssh:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "amqps?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "mqtt:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "kafka:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "s3:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "gs:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "azure:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "mailto:"
                },
                {
                  "type": "PATTERN",
                  "value": "data:"
                },
                {
                  "type": "PATTERN",
                  "value": "ldaps?:\\/\\/"
                },
                {
                  "type": "PATTERN",
                  "value": "urn:"
                }
              ]
            },
            {
              "type": "PATTERN",
              "value": "[^\\s]+"
            }
          ]
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
