{
  "name": "env",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_spacing"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_spacing"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "_spacing": {
      "type": "PATTERN",
      "value": "[ \\t]*"
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_double"
        },
        {
          "type": "SYMBOL",
          "name": "string_single"
        },
        {
          "type": "SYMBOL",
          "name": "raw_value"
        }
      ]
    },
    "raw_value": {
      "type": "PATTERN",
      "value": "[^\"\\'\\n\\r][^\\n\\r]*"
    },
    "string_double": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "SYMBOL",
                "name": "interpolation"
              },
              {
                "type": "PATTERN",
                "value": "[^\"$\\\\\\\\]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "string_single": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "PATTERN",
                "value": "[^'\\\\\\\\]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "interpolation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "interpolation_default"
        },
        {
          "type": "SYMBOL",
          "name": "interpolation_simple"
        },
        {
          "type": "SYMBOL",
          "name": "interpolation_short"
        }
      ]
    },
    "interpolation_default": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "${"
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "STRING",
                  "value": "-"
                }
              ]
            }
          },
          {
            "type": "FIELD",
            "name": "default",
            "content": {
              "type": "SYMBOL",
              "name": "interpolation_value"
            }
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "interpolation_simple": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "interpolation_short": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "interpolation_value": {
      "type": "PATTERN",
      "value": "[^}]+"
    },
    "escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\."
    },
    "identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "@"
            },
            {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_-]+"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_-]+"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
