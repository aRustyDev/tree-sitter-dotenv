# Test file for primitive value types in .env format

# Boolean values
ENABLED=true
DISABLED=false
DEBUG_MODE=true
PRODUCTION=false

# Boolean edge cases
BOOL_UPPERCASE=TRUE    # Should be parsed as raw string, not boolean
BOOL_MIXED_CASE=True   # Should be parsed as raw string, not boolean
BOOL_TYPO=ture         # Should be parsed as raw string, not boolean
BOOL_WITH_SPACE=true   # Note the space after true

# Integer values
PORT=3000
TIMEOUT=60
MAX_CONNECTIONS=100
MIN_VALUE=0
NEGATIVE_VALUE=-42
ZERO=0

# Integer edge cases
INT_WITH_SPACES=123    # Spaces after number
LEADING_ZEROS=0123     # Should preserve leading zeros
LARGE_NUMBER=9999999999
INT_WITH_SUFFIX=123px  # Should be parsed as raw value, not integer

# Mixed types in one file
STRING_VALUE="localhost"
BOOL_VALUE=true
INT_VALUE=8080
RAW_VALUE=some-raw-value

# Values that could be ambiguous
TRUE_STRING="true"     # String, not boolean
FALSE_STRING='false'   # String, not boolean  
NUMBER_STRING="3000"   # String, not integer
ZERO_STRING="0"        # String, not integer